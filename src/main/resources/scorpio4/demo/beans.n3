@prefix xsd: 		<http://www.w3.org/2001/XMLSchema#>.
@prefix bean: 		<http://scorpio4.com/core/bean/>.

# Define Outer Bean
<bean:com.scorpio4.demo.Example1>			a 			bean:Bean.

# Define Inner Bean
<bean:com.scorpio4.demo.Example1_1> 		a 			bean:Bean.

# Define an instance of our Outer Bean
<urn:com.scorpio4.demo.Example1>			a 			<bean:com.scorpio4.demo.Example1>;
# Inject a simple String and a reference to our Inner Bean
	bean:new	("Hello World" <bean:com.scorpio4.demo.Example1_1>);
.

